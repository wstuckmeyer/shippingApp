
<h1>List ship</h1>

 <table>
  <thead>
    <tr>
      <th>Containers</th><br />
      <th>Location</th><br />
      <th>User ID</th><br />
      <th colspan="3"></th><br />
    </tr>
  </thead>

  <tbody>
    <% @ship.each do |ship| %>
      <tr>
        <td><%= ship.name %></td>
        <td><%= ship.containers %></td>
        <td><%= ship.location %></td>
        <td><%= ship.user_id %></td>
        <td><%= link_to 'Show', ship %></td>
        <td><%= link_to 'Edit', edit_ship_path(ship) %></td>
        <td><%= link_to 'Delete', ship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h2><%= @createvenue.name %></h2>
