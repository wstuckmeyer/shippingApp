<h1>New Ships</h1>

	<%= form_for :ship, url: ships_path do |f| %>
		<p>
			<%= f.label :user_id %><br>
			<%= f.text_field :user_id %><br>
		</p>
		<p>
			<%= f.label :name %><br>
			<%= f.text_field :containers %><br>
		</p>
		<p>
			<%= f.label :location %><br>
			<%= f.text_field :location %><br>
		</p>
		<p>
			<%= f.submit %>
		</p>

<% end %>